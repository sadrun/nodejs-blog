<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title><%= title %></title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
	<link rel="stylesheet" href="/css/mobile/slide.css">
</head>
<body>
	<div id="pages">
		<div class="page p1">
			<div class="bg">
				<img src="/images/mobile/bg1.jpg" width="100%" height="100%">
			</div>
			<div class="pulldown"></div>
		</div>
		<div class="page p2">
			<div class="bg">
				<img src="/images/mobile/bg2.jpg" width="100%" height="100%">
			</div>
		</div>
		<div class="page p3">
			<div class="bg">
				<img src="/images/mobile/bg3.jpg" width="100%" height="100%">
			</div>
			<div class="music-icon"></div>
		</div>
		<div class="page p4">
			<div class="bg">
				<img src="/images/mobile/bg4.jpg" width="100%" height="100%">
			</div>
		</div>
		<div class="page p5" style="display:none">
			<div class="bg">
				<img src="/images/mobile/bg5.jpg" width="100%" height="100%">
			</div>
			<div class="space-star-box">
				<div class="space-star ss1"></div>
				<div class="space-star ss2"></div>
			</div>
		</div>
	</div>

	<script type="text/javascript" src="/js/mobile/iSlider.js"></script>
	<script type="text/javascript">
		var _height =document.body.offsetHeight;
		document.querySelector('#pages').style.height = _height+'px';
		new ISlider({container:"pages"});
	</script>
</body>
</html>